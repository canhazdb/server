{
  "name": "canhazdb",
  "version": "2.0.7",
  "description": "A shaded and clustered database communicated over http rest.",
  "main": "./index.js",
  "bin": {
    "canhazdb": "server/cli.js"
  },
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "chalk": "^4.1.0",
    "final-stream": "^2.0.2",
    "get-port": "^5.1.1",
    "hinton": "^1.0.0",
    "lockbase": "^1.0.9",
    "minimist": "^1.2.5",
    "mongo-sql": "^6.2.0",
    "mql-to-jql": "^1.1.8",
    "node-ejdb-lite": "^3.1.4",
    "qs": "^6.9.4",
    "reconnecting-websocket": "^4.4.0",
    "server-destroy": "^1.0.1",
    "sqlite": "^4.0.15",
    "sqlite3": "^5.0.0",
    "tcpocket": "^4.0.0",
    "uuid": "^8.3.1",
    "write-response": "^1.2.3",
    "ws": "^7.4.0"
  },
  "devDependencies": {
    "basictap": "^1.1.3",
    "c8": "^7.3.3"
  },
  "scripts": {
    "test": "./makeCerts.sh && npm run test:ejdb && npm run test:sqlite",
    "test:ejdb": "CANHAZDB_DRIVER=ejdb c8 -o /tmp node --unhandled-rejections=strict test",
    "test:sqlite": "CANHAZDB_DRIVER=sqlite c8 -o /tmp node --unhandled-rejections=strict test"
  },
